/^[a-z\d.-]+:\/\//i,quotes={"'":"`",">":"<",")":"(","]":"[","}":"{","\u00bb":"\u00ab","\u203a":"\u2039"},default_options={callback:function(text,href){return href?'<a href="'+href+'" title="'+href+'">'+text+"</a>":text},punct_regexp:/(?:[!?.,:;'"]|(?:&|&amp;)(?:lt|gt|quot|apos|raquo|laquo|rsaquo|lsaquo);)$/};return function(txt,options){options=options||{};var arr,i,html="",parts=[],quote_begin;for(i in default_options)void 0===options[i]&&(options[i]=default_options[i]);for(;arr=URI_RE.exec(txt);){var link=
