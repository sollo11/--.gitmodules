goog.i18n.bidi.guardBracketInText=function(s,opt_isRtlContext){const useRtl=void 0===opt_isRtlContext?goog.i18n.bidi.hasAnyRtl(s):opt_isRtlContext,mark=useRtl?goog.i18n.bidi.Format.RLM:goog.i18n.bidi.Format.LRM;return s.replace(goog.i18n.bidi.bracketGuardTextRe_,mark+"$&"+mark)};goog.i18n.bidi.enforceRtlInHtml=function(html){return"<"==html.charAt(0)?html.replace(/<\w+/,"$& dir=rtl"):"\n<span dir=rtl>"+html+"</span>"};
