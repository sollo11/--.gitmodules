statuses=$.grep(statuses,function(status){return currentStatus!=status;});var context={'statuses':statuses,'description':parser.getMessage('description'),'reportHints':parser.getMessage('reportHints'),'statusNames':parser.getMessage('statusNames')};commandUtils.showPrompt(evt,'change-idea-status-dialog',context,{beforeShow:function($dialog){var $statusSelectInput=$dialog.find("select[name='status']");var $reasonSelectInput=$dialog.find("select[name='reason']");$statusSelectInput.change(function(e){var status=$(e.target).val();if(reportHints[status].length>0){$dialog.find(".reasons-container select[name='reason']").empty();$.each(reportHints[status],function(key,value){$dialog.find(".reasons-container select[name='reason']").append('<option value="'+value.name+'">'+value.label+'</option>');});$dialog.find(".reasons-container").show();$dialog.find(".reasons-container select[name='reason']").trigger('change');}else{$("select[name='reason']").val('');$dialog.find(".reasons-container").hide();$dialog.find(".other-comments-container").hide();$dialog.find(".other-comments-container textarea").val('');}});$reasonSelectInput.change(function(e){var currentReason=$(e.target).val();var $statusSelectInput=$dialog.find("select[name='status']");var shouldShowExtraComments=false;$.each(reportHints[$statusSelectInput.val()],function(key,value){if(currentReason==value.name&&value.allowExtraComments=="true")
shouldShowExtraComments=true;});if(shouldShowExtraComments){$dialog.find(".other-comments-container").show();}else{$dialog.find(".other-comments-container").hide();$dialog.find(".other-comments-container textarea").val('');}});if(reportHints[$statusSelectInput.val()].length>0){$dialog.find(".reasons-container select[name='reason']").empty();$.each(reportHints[$statusSelectInput.val()],function(key,value){$dialog.find(".reasons-container select[name='reason']").append('<option value="'+value.name+'">'+value.label+'</option>');});$dialog.find(".reasons-container").show();var currentReason=$dialog.find(".reasons-container select[name='reason']").val();var shouldShowExtraComments=false;var shouldShowExtraComments=false;$.each(reportHints[$statusSelectInput.val()],function(key,value){if(currentReason==value.name&&value.allowExtraComments=="true")
shouldShowExtraComments=true;});if(shouldShowExtraComments){$dialog.find(".other-comments-container").show();}else{$dialog.find(".other-comments-container").hide();$dialog.find(".other-comments-container textarea").val('');}}else{$dialog.find(".reasons-container").hide();$dialog.find(".other-comments-container textarea").val('');$dialog.find(".other-comments-container").hide();}},onOk:function(data){parser.executeRequest(data);}});},onSuccess:function(data,evt){this.setStatus(data.result.setStatusAction.extra+","+data.result.setStatusActionReason);},onCompleted:function(success,evt){if(success){this.updateTitle();}},updateTitle:function(){if(this.getStatus()!="none"){var res=this.getStatus().split(",")
var status=res[0];var reason=res[1];var shouldShowReason=this.shouldShowReason?this.shouldShowReason:globalShouldShowReason;$("#id_tags_container .idea-status-label").remove();if(status!='none'){var text='<a href="#" class="idea-status-label label '+status+'">'+commands.ideaStatus.getMessage('statusNames')[status]
if(shouldShowReason&&reason&&reason!="none"){text+=' - '+reason}
text+='</a>'
$("#id_tags_container").prepend($(text));}}}});register_command('viewIdeaStateHistoryCommand',{getRequestMethod:function(){return'GET';},onSuccess:function(data,evt){var parser=this;var context={'history':data.result.history,'description':parser.getMessage('description')};commandUtils.showPrompt(evt,'view-idea-status-history-dialog',context,{title:"Idea Status History",show_yes:false,show_no:false});},onCompleted:function(success,evt){}});