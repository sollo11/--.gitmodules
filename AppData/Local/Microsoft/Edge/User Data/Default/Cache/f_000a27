$(document).ready(function(){$(document).on('click','.node-attachment-delete',function(){var $link=$(this);var fileId=$link.parent().find('input[type=hidden]').val();$.getJSON(pageContext.url.cancelUpload,{file:fileId});$link.parent().fadeOut('fast',function(){$link.parent().remove();if($(".img-node-attachment").length<1&&$(".node-attachment").length<1){$('#main-attachments-container').slideUp();}});$('.redactor_editor #'.concat(fileId)).remove();$('.redactor').redactor('insertText','');$('.commentBox').redactor('insertText','');return false;});$('.widget-content form').each(function(i,e){$(e).submit(function(ev){var box=$(e).find('.redactor')||null;if(box!==null&&box.is('textarea')){replaceAttachmentUrl(box);}});});});$(function(){if(pageContext.additional.recaptchaKey){var $submit=$('form:has(#body)').find('input[type=submit], input[type=button], button').first();commandUtils.getScript('https://www.google.com/recaptcha/api.js',function(){$submit.before('<div class="g-recaptcha" data-sitekey="'+pageContext.additional.recaptchaKey+'"></div>');if(pageContext.additional.recaptchaError){$submit.before('<div style="color: #B94A48;">'+pageContext.additional.recaptchaError+'</div> ');}});}});register_command('voteInList',{onSuccess:function(data){$('#post-'+this.nodeId+'-score-wrapper').html(data.result.score);this.getStatus()!="on"?this.element.addClass("btn-success"):this.element.removeClass("btn-success");$("#refresh-votes").click();}});register_command('voteUpInList',{onSuccess:function(data){$('#post-'+this.nodeId+'-score-wrapper').find('span').html(data.result.score);$("#idea-vote-down-"+this.nodeId).removeClass('on');$("#refresh-votes").click();}});register_command('voteDownInList',{onSuccess:function(data){$('#post-'+this.nodeId+'-score-wrapper').find('span').html(data.result.score);$("#idea-vote-up-"+this.nodeId).removeClass('on');$("#refresh-votes").click();}});register_command('ideaStatus',{execute:function(evt){var parser=this;var currentStatus=parser.getStatu