<script type="text/javascript">
    $('document').ready(function(){
        $('#commentVisibility li a').on("click", function(){
            var form = $(this).closest("form");
            form.attr("action", form.attr("action").replace("addApigeeksOnly.json", "add.json"));
            if($(this).attr("id") === "apigeeksOnly"){
                $('.comment-recipients-label').text("Viewable by Apigeeks Only")
                form.attr("action", form.attr("action").replace("add.json","addApigeeksOnly.json"));
            }
        });
    });
</script>

