<script type="text/javascript">
    var searchItemMouseDown = false;
    var $searchAuto = $("#search-query").autocomplete({
        open: function () {
            $searchAuto.addClass("open");
            $("#searchResultsAutoComplete").find("a").mousedown(function (e) {
                searchItemMouseDown = true;
            });
        },
        close: function (e) {
            $searchAuto.removeClass("open");
        },
        appendTo: '#searchResultsAutoComplete',
        autoFocus: false,
        highlight: function (match, keywords) {
            keywords = keywords.split(' ').join('|');
            return match.replace(new RegExp("(" + keywords + ")", "gi"), '<b>$1</b>');
        },
        source: function (request, response) {
            var query = request.term;
            var labelText = $('<div/>').text(request.term).html();

            if (!/\s$/.test(query)) {
                query += "*";
            }

            $.ajax({
                url: '/search.json',
                dataType: 'json',
                minLength: 0,
                delay: 30,
                data: { q: query, page: 1, pageSize: 7, type: "object", compact: 'false' },
                success: function (data) {
                    try {
                        externalResults = data.externalSearchResults instanceof Array ? data.externalSearchResults[0].results : data.externalSearchResults.results;
                        mixedResults = $.merge(data.searchResults.results, externalResults);
                    } catch(e) {
                        mixedResults = data.searchResults.results
                    }
                    var $results = $.map(mixedResults, function (item, index) {
                        if (item != null) {
                        	if (item.object){
                                return {
                                        label: item.object.title.replace(
                                                new RegExp(
                                                        "(?![^&;]+;)(?!<[^<>]*)(" +
                                                                $.ui.autocomplete.escapeRegex(request.term) +
                                                                ")(?![^<>]*>)(?![^&;]+;)", "gi"
                                                ), "<strong>$1</strong>"),
                                        url: item.object.link,
                                        value: item.object.title,
                               };
                            }
                            if (item.username){
                                var name = item.username;
                                if(item.realname){
                                    name = item.realname;
                                }
                                return {

                                    label: name.replace(
                                            new RegExp(
                                                            "(?![^&;]+;)(?!<[^<>]*)(" +
                                                            $.ui.autocomplete.escapeRegex(request.term) +
                                                            ")(?![^<>]*>)(?![^&;]+;)", "gi"
                                            ), "<strong>$1</strong>"),
                                    url: "/users/%7Bid%7D/%7Bplug%7D.html", //item.id
                                    value: name,
                                    id: item.id,
                                    plug: item.plug,
                                    type: "User",
                                    emailHash: item.emailHash
                                };
                            }
                            if (typeof item.title !== 'undefined')
                            {
                                switch (item.type) {
                                    case 'topic':
                                        return {
                                            label: item.name.replace(
                                                    new RegExp(
                                                            "(?![^&;]+;)(?!<[^<>]*)(" +
                                                                    $.ui.autocomplete.escapeRegex(request.term) +
                                                                    ")(?![^<>]*>)(?![^&;]+;)", "gi"
                                                    ), "<strong>$1</strong>"),
                                            url: "/topics/%7Bid%7D.html", //item.id
                                            value: item.name,
                                            id: item.name,
                                            plug: item.plug,
                                            type: "Topic"
                                        };
                                    case 'user':
                                        var name = item.username;
                                        if(item.realname){
                                            name += " | " + item.realname;
                                        }
                                        return {

                                            label: name.replace(
                                                    new RegExp(
                                                            "(?![^&;]+;)(?!<[^<>]*)(" +
                                                                    $.ui.autocomplete.escapeRegex(request.term) +
                                                                    ")(?![^<>]*>)(?![^&;]+;)", "gi"
                                                    ), "<strong>$1</strong>"),
                                            url: "/users/%7Bid%7D/%7Bplug%7D.html", //item.id
                                            value: name,
                                            id: item.id,
                                            plug: item.plug,
                                            type: "User",
                                            emailHash: item.emailHash
                                        };
                                    case 'question':
                                        return {
                                            label: item.title.replace(
                                                    new RegExp(
                                                            "(?![^&;]+;)(?!<[^<>]*)(" +
                                                                    $.ui.autocomplete.escapeRegex(request.term) +
                                                                    ")(?![^<>]*>)(?![^&;]+;)", "gi"
                                                    ), "<strong>$1</strong>"),
                                            url: "/questions/%7Bid%7D/%7Bplug%7D.html", //item.id
                                            value: item.title,
                                            id: item.id,
                                            plug: item.plug,
                                            type: item.type
                                        };
                                    default:
                                        return {
                                            label: item.title.replace(
                                                    new RegExp(
                                                            "(?![^&;]+;)(?!<[^<>]*)(" +
                                                                    $.ui.autocomplete.escapeRegex(request.term) +
                                                                    ")(?![^<>]*>)(?![^&;]+;)", "gi"
                                                    ), "<strong>$1</strong>"),
                                            url: "/content/%7Btype%7D/%7Bid%7D/%7Bplug%7D.html", //item.id
                                            value: item.title,
                                            id: item.id,
                                            plug: item.plug,
                                            type: item.type
                                        };
                                }
                            }
                        }
                    });
                    $results.push({
                        label: "Search: " + "\"" + $.trim(labelText) + "\"",
                        url: "/search.html?q={query}",
                        query: request.term,
                        value: request.term
                    });
                    $results.push({
                        label: "Add Question: <strong>\"$q\"<\/strong>".replace("$q", $.trim(labelText)),
                        url: "/questions/ask.html?a=true&q={query}&space=13",
                        query: request.term,
                        value: request.term
                    });

                    response($results);
                }
            });
        },
        select: function (event, ui) {
            var url = ui.item.url.replace("%7Bid%7D", ui.item.id);
            url = url.replace("%7Bplug%7D", ui.item.plug);
            url = url.replace("{query}", ui.item.query);
            url = url.replace("%7Btype%7D", ui.item.type);
            window.location = url;
            return false;
        }
    }).keydown(function () {
                $(this).css('color', '#000');
            });

    if ($searchAuto.length) {
        $searchAuto.data("uiAutocomplete")._renderItem = function (ul, item) {
            var type_str = "";
            if (item.type == "Topic")
                    type_str = "<span class='autocomplete-type'>" + "Topic" + "</span>";
            return $("<li></li>")
                    .data("item.autocomplete", item)
                    .append("<a>" + (item.type == 'User' ? '<img class="gravatar" align="left" width="16" height="16" style="margin-right: 5px;" src="//www.gravatar.com/avatar/'
                            + item.emailHash + '?s=16&amp;d=identicon&amp;r=PG"/>' : '') + " " + item.label + " " + type_str + "</a>")
                    .appendTo(ul);
        };
    }
</script><script type="text/javascript">

    $(document).ready(function () {
        $bottom = $("#bottom");
        $notFooter = $("#not-footer");
        $push = $(".push");
        $push.css("height", $bottom.outerHeight() + "px");
        $notFooter.css("margin-bottom", "-" + $bottom.outerHeight() + "px");

        $(".dummy-navbar-link").sidr();

        $(".navbar-toggle").on("click", function(){
            $.sidr(
                "open",
                "sidr",
                function(){
                    $(document.body).prepend("<div class='cover'></div>");
                    $(".cover").fadeIn().on("click", function(){
                        $.sidr('close', 'sidr');
                        $(this).remove();
                    });
                }
            );
        });
        
        $('.kbentry-container .show-comments-container').click();
    });


    $(".answer-body pre").addClass("prettyprint");
    $(".node-body pre").addClass("prettyprint");
    $(".question-body pre").addClass("prettyprint");
    prettyPrint();

    $(document).ajaxComplete(function( event, xhr, settings ) {
        if (settings.url.indexOf("condensedProfile.json") >= 0) {
            var uid = settings.url.split("/")[2];
            $.get('/questions/isApigeek.json', {user: uid}, function( data ) {
                if (data.result.isApigeek == true) {
                    $('#qtip-' + settings.context.id + '-content').find('.nameAndTagline').append('<span class="apigeek">Apigeek</span>');
                    $('#qtip-' + settings.context.id + '-content').find('.location').hide();
                }
            });
            $.get('/academyAward/hasAcademyAward.json', {user: uid}, function( data ) {
                if (data.result.data == true) {
                    $('#qtip-' + settings.context.id + '-content').find('ul.stats').append('<li><a href="http://academy.apigee.com/courses/certification" target="_blank"><img src="/plugins/resources/apigee/images/badges/com.teamhub.plugins.apigee.awards.academy.name.png" alt="" /></a></li>');
                }
            });
        }

        if (settings.url.indexOf("add.json") >= 0) {
            var json = JSON.parse(xhr.responseText);
            $.get('/academyAward/hasAcademyAward.json', {user: json.comment.author.id}, function( data ) {
                if (data.result.data == true) {
                    $('#comment-' + json.comment.id).find('a.comment-user').append('&nbsp;<a href="http://academy.apigee.com/courses/certification" target="_blank"><img src="/plugins/resources/apigee/images/badges/com.teamhub.plugins.apigee.awards.academy.name.png" height="15px" width="15px" alt="" /></a>');
                }
            });
        }

        if (settings.url.indexOf("list.json") >= 0) {
            var json = JSON.parse(xhr.responseText);
            json = json.result.replies;
            $.each(json.comments, function(i, item) {
                $.get('/academyAward/hasAcademyAward.json', {user: item.author.id}, function( data ) {
                    if (data.result.data == true) {
                        $('#comment-' + item.id).find('a.comment-user').append('&nbsp;<a href="http://academy.apigee.com/courses/certification" target="_blank"><img src="/plugins/resources/apigee/images/badges/com.teamhub.plugins.apigee.awards.academy.name.png" height="15px" width="15px" alt="" /></a>');
                    }
                });
            })
        }
    });

</script>
</body>
