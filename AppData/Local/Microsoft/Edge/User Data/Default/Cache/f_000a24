<script type="text/javascript">
    $(document).ready(function () {
		// var ie7 = $.browser.msie && $.browser.version <= 7;

        var label = $('.navbar-search label[for=search_box]')
        label.css({
            position: 'absolute',
            top: 4,
            left: 17,
            zindex: 10
        });
        $('#search_box')
        .focus(function () {
            label.hide()
        })
        .blur(function () {
            this.value == "" ? label.show() : label.hide()
        })

        if(!/^(?:\/?index\.html|\/)$/i.test(location.pathname)){
        	var searchBottomSpacing = parseFloat($('#main-navbar .navbar-inner .container').css('margin-bottom').replace(/[^\d\.\,]+/gi,''));
        	$('.welcome-txt,.upcoming-txt').remove();
        	$('#main-navbar .navbar-inner').first().css('margin-top',(searchBottomSpacing/2)+'px');
        }
    });
</script>

