        <script type="text/javascript">
  function handleCrossTabLogout(event) {
    if (typeof localStorage != "undefined" && event && event.key === "logout-action" && event.newValue === "true") {
      window.isLoggedIn = false;
      console.log("StorageEvent :: handleCrossTabLogout :: values received ====> ", event.key, event.oldValue, event.newValue);
      console.log("Setting window.isLoggedIn flag as false...");
      localStorage.removeItem("logout-action");
    }
  }
  console.log("Adding logout-action event handler...");
  window.addEventListener("storage", handleCrossTabLogout);
</script>

