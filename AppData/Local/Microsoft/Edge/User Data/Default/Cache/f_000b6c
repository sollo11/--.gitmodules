            <script type="text/javascript">

                function showHelp() {
                    var divHelp = document.getElementById("divHelp");
                    divHelp.style.display = "";
                }

                function redirect(url) {
                    var safeUrl = url;

                    //We must all redirects on the public site to stay with /Europe 
                    //If redirecting into SMART, we must NOT add /Europe as /Europe does not contain a SMART virtual directory
                    if (parent.location.href.toLowerCase().indexOf("/europe/") > -1 && url.toLowerCase().indexOf("/smart") === -1) {
                        safeUrl = "/Europe" + safeUrl;
                    }

                    //We must all redirects on the public site to stay with /India 
                    //If redirecting into SMART, we must NOT add /India as /India does not contain a SMART virtual directory
                    if (parent.location.href.toLowerCase().indexOf("/india/") > -1 && url.toLowerCase().indexOf("/smart") === -1) {
                        safeUrl = "/India" + safeUrl;
                    }

                    if (parent.parentExists) {
                        parent.redirect(safeUrl);
                    }
                    else {
                        window.top.location = safeUrl;
                    }
                }
                function openWindow(path) {
                    Window_Open = window.open(path, 'OpenWindow', 'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1,copyhistory=0,width=850,height=715,top=25,left=300');
                    Window_Open.focus();
                }
                $('#ftr .popUpLink').click(function (e) {
                    e.preventDefault();
                    openWindow(this.href);
                    return false;
                });
            </script>
        
